              var StateADS = "Menu";
          $(document).ready(function(){
            $(window).on('resize', function(){
            var width = $(window).width();
                if(StateADS == "Menu")
                    {
                           if(width < MyWidth)
                            {
                                 $("#scaryclown-ru_300x250").css("z-index", "-2");
                            }
                        else
                            {
                                 $("#scaryclown-ru_300x250").css("z-index", "2");
                            }
                    }
                if(StateADS == "Pause")
                    {
                             if(width < MyWidth)
                            {
                                 $("#scaryclown-ru_300x600").css("z-index", "-2");
                            }
                        else
                            {
                                 $("#scaryclown-ru_300x600").css("z-index", "2");
                            }
                    }
                   if(StateADS == "Winner")
                    {
                           if(width < MyWidth)
